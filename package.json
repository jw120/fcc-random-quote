{
    "name": "random-quote",
    "version": "1.0.0",
    "description": "FreeCodeCamp random quote generator exercise",
    "main": "index.js",
    "config": {
        "my_src": "compiled/src/random-quote.js src/random-quote.html src/random-quote.css",
        "lib_src": "node_modules/bootstrap/dist/css/bootstrap.min.css node_modules/bootstrap/dist/js/bootstrap.min.js node_modules/jquery/dist/jquery.min.js"
    },
    "scripts": {
        "lint": "eslint src",
        "precompile": "npm run lint",
        "compile": "babel src/random-quote.js --out-dir compiled/",
        "prebuild": "npm run compile",
        "build": "cp $npm_package_config_my_src $npm_package_config_lib_src dist/",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "jw1200@gmail.com",
    "license": "MIT",
    "dependencies": {
        "bootstrap": "^3.3.6",
        "jquery": "^2.2.3"
    },
    "devDependencies": {
        "babel": "^6.5.2",
        "babel-cli": "^6.8.0",
        "babel-preset-es2015": "^6.6.0"
    }
}
